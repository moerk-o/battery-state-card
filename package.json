{
  "name": "battery-state-card",
  "version": "3.2.1",
  "description": "Battery State card for Home Assistant",
  "main": "dist/battery-state-card.js",
  "author": "Max Chodorowski",
  "license": "MIT",
  "keywords": [
    "battery",
    "state",
    "home",
    "assistant",
    "lovelace"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maxwroc/battery-state-card.git"
  },
  "scripts": {
    "build": "rollup -c",
    "release": "rollup --environment RELEASE -c",
    "watch": "rollup -c --watch",
    "test": "NODE_OPTIONS=\"--no-experimental-fetch\" jest",
    "test+integration": "NODE_OPTIONS=\"--no-experimental-fetch\" jest --testPathPatterns=\"test/(entity|card)\"",
    "test+coverage": "NODE_OPTIONS=\"--no-experimental-fetch\" jest --coverage",
    "test+coverage+unit": "NODE_OPTIONS=\"--no-experimental-fetch\" jest --coverage --testPathPatterns=test/other",
    "test+unit": "NODE_OPTIONS=\"--no-experimental-fetch\" jest --testPathPatterns=test/other",
    "test+debug": "SET DEBUG_MODE=1&&jest"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^2.79.1",
    "rollup-plugin-import-css": "^3.3.5",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-version-injector": "^1.3.3",
    "ts-jest": "^29.2.0",
    "tslib": "^2.5.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "custom-card-helpers": "^1.9.0",
    "lit": "^2.8.0"
  }
}
